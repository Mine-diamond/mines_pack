{
    "type": "minecraft:multi_action",
    "button_width": 310,
    "columns": 2,
    "external_title": "快捷操作",
    "title": "快捷操作",
    "body": [
    {
      "type": "plain_message",
      "contents": {
        "text": "超多快捷操作",
        "color": "white"
      }
    }
  ],
    "actions": [
        {
            "label": "设为白天",
            "on_click": {
                "action": "run_command",
                "command": "/time set noon"
            }
        },
        {
            "label": "设为黑夜",
            "on_click": {
                "action": "run_command",
                "command": "/time set midnight"
            }
        },
        {
            "label": "清理掉落物",
            "on_click": {
                "action": "run_command",
                "command": "/kill @e[type=item]"
            }
        },
        {
            "label": "清理非玩家实体",
            "on_click": {
                "action": "show_dialog",
                "dialog": "names:kill_all_mob_comfirm"
            }
        },
        {
            "label": "玩家无敌",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:invulnerable"
            }
        },
        {
            "label": "取消玩家无敌",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:un_invulnerable"
            }
        },
        {
            "label": "一击必杀",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:one_click_kill"
            }
        },
        {
            "label": "取消一击必杀",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:cancel_one_click_kill"
            }
        },
        {
            "label": "赶路模式",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:world_run_mode"
            }
        },
        {
            "label": "取消赶路模式",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:cancel_world_run_mode"
            }
        },
        {
            "label": "开启时间显示",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:time/time_show"
            }
        },
        {
            "label": "关闭时间显示",
            "on_click": {
                "action": "run_command",
                "command": "/execute as @s run function names:time/time_hide"
            }
        },
        {
            "label": "高亮实体",
            "on_click": {
                "action": "run_command",
                "command": "/effect give @e minecraft:glowing infinite 1 true"
            }
        },
        {
            "label": "取消高亮实体",
            "on_click": {
                "action": "run_command",
                "command": "/effect clear @e minecraft:glowing"
            }
        },
        {
            "label": {
                "text": "常见设置",
                "color": "yellow"
            },
            "on_click": {
                "action": "show_dialog",
                "dialog": "names:usual_setting"
            }
        },
        {
            "label": {
                "text": "数据包仓库链接",
                "color": "aqua"
            },
            "on_click": {
                "action": "open_url",
                "url": "https://github.com/Mine-diamond/mines_pack"
            },
            "tooltip": "https://github.com/Mine-diamond/mines_pack"
        }
    ]
}
